{
  "$schema": "http://json-schema.org/draft-07/schema",
  "description": "A Button for clicking!",
  "type": "object",
  "title": "Button",
  "required": [],
  "properties": {
    "type": {
      "type": "string",
      "title": "type",
      "default": "primary",
      "enum": [
        "primary",
        "secondary",
        "success",
        "danger",
        "warning",
        "info",
        "light",
        "dark",
        "link"
      ],
      "enumNames": [
        "Primary",
        "Secondary",
        "Success",
        "Danger",
        "Warning",
        "Info",
        "Light",
        "Dark",
        "Link"
      ]
    },
    "size": {
      "type": "string",
      "title": "Size",
      "default": "md",
      "enum": [
        "sm",
        "md",
        "lg"
      ],
      "enumNames": [
        "Small",
        "Default",
        "Large"
      ]
    },
    "disabled": {
      "type": "boolean",
      "title": "Disabled",
      "default": false
    },
    "outlined": {
      "type": "boolean",
      "title": "Outlined",
      "default": false
    },
    "handleClick": {
      "description": "This is sick",
      "typeof": "function",
      "tsType": "() => void"
    },
    "children": {
      "typeof": "function",
      "tsType": "React.ReactNode"
    },
    "icon": {
      "typeof": "function",
      "tsType": "React.ReactNode",
      "description": "undefined. Only use: button"
    }
  }
}