{
  "name": "@knapsack/example-bootstrap",
  "license": "ISC",
  "version": "2.0.0-beta.78",
  "description": "",
  "private": true,
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production run-s --print-name clean build:tokens build:wp build:knapsack",
    "build:knapsack": "NODE_ENV=production knapsack build",
    "build:tokens": "style-dictionary build --config ./style-dictionary.config.js",
    "build:wp": "webpack",
    "clean": "rm -rf ./assets/design-tokens/dist",
    "serve": "NODE_ENV=production knapsack serve",
    "serve:test": "KS_TEST_RUN=yes knapsack serve",
    "start": "yarn prep && run-p watch:*",
    "watch:wp": "yarn build:wp --watch",
    "prep:wp": "[ -f public/dist/main.js ] || yarn build:wp",
    "prep:tokens": "[ -f assets/design-tokens/dist/knapsack-design-tokens.json ] || yarn build:tokens",
    "prep": "run-p prep:*",
    "watch:tokens": "nodemon --watch ./assets/design-tokens --ext js --ignore ./assets/design-tokens/dist --exec 'yarn build:tokens'",
    "watch:knapsack": "knapsack start",
    "test": "knapsack test"
  },
  "dependencies": {
    "@knapsack/app": "2.0.0-beta.78",
    "@knapsack/babel-config": "2.0.0-beta.78",
    "@knapsack/build-tools": "2.0.0-beta.78",
    "@knapsack/plugin-changelog-md": "2.0.0-beta.78",
    "@knapsack/renderer-html": "2.0.0-beta.78",
    "@knapsack/renderer-react": "2.0.0-beta.78",
    "@knapsack/renderer-twig": "2.0.0-beta.78",
    "@knapsack/renderer-web-components": "2.0.0-beta.78",
    "classnames": "^2.2.6",
    "lit-element": "^2.2.1",
    "lodash.template": "^4.4.0",
    "nodemon": "^2.0.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "style-dictionary": "^2.7.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  }
}
